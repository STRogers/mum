{
    "name": "example",
    "install": {
        "map": [
            {
                "source": "./config",
                "destination": "../config"
            },
            {
                "source": "./project",
                "destination": "./"
            },
            {
                "source": "./vendor",
                "destination": "./vendor"
            }
        ],
        "scripts": {
            "before": ["./scripts/prepare"],
            "after": ["./scripts/after.php"]
        }
    },
    "update": {
        "scripts": {
            "before": ["./scripts/prepare.php"],
            "after": ["./scripts/after.php"]
        }
    },
    "dependencies": [
        {
            "name": "mum-example",
            "url": "git@github.com:mwhite05/mum-example-js.git#master",
            "version": "0.1.0",
            "installTo": "./js"
        },
        {
            "name": "mum-example-config",
            "url": "git@github.com:mwhite05/mum-example-js.git#config",
            "version": "0.1.0",
            "installTo": "./js"
        }
    ]
}
