{
    "name": "example",
    "install": {
        "map": [
            {
                "source": "./config",
                "destination": "../config"
            },
            {
                "source": "./project",
                "destination": "./"
            },
            {
                "source": "./vendor",
                "destination": "./vendor"
            }
        ],
        "scripts": {
            "before": ["./scripts/prepare"],
            "after": ["./scripts/after.php"]
        }
    },
    "update": {
        "scripts": {
            "before": ["./scripts/prepare.php"],
            "after": ["./scripts/after"]
        }
    },
    "dependencies": [
        {
            "name": "mum-example",
            "source": "git@github.com:mwhite05/mum-example-js.git#master",
            "installTo": "./js",
            "exclude": ["config"],
            "config": {
                "map": [
                    {
                        "source": "",
                        "destination": ""
                    }
                ]
            }
        }
    ]
}
